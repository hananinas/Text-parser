    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch Main",
            "request": "launch",
            "mainClass": "bfst2023.handins.Main",
            "vmArgs": "--module-path C:/java/javafx-sdk-19/lib --add-modules javafx.controls,javafx.fxml",
            "projectName": "app"
        },
        {
            "type": "java",
            "name": "Launch Current File",
            "vmArgs": "--module-path C:/java/javafx-sdk-19/lib --add-modules javafx.controls,javafx.fxml",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Launch App",
            "request": "launch",
            "vmArgs": "--module-path C:/java/javafx-sdk-19/lib --add-modules javafx.controls,javafx.fxml",
            "mainClass": "bfst2023.handins.App",
            "projectName": "app"
        }
    ]
}